mkdir -p D1/D2/D3
cd D1/D2/
touch file.txt
vi file.txt
ln D1/D2/file.txt D1/D2/D3/hardlink.txt
ln -s D1/D2/file.txt D1/D2/D3/link.txt
mv D1/D2/file.txt D1/
ls -l D1/
ls -l D1/D2
cat D1/D2/D3/hardlink.txt
cat D1/D2/D3/link.txt
rm -r D1/D2/
find / -type f -size +20M 2>/dev/null > "${USER}_part3_gt50M.log"
du -h $(cat "${USER}_part3_gt50M.log") 2>/dev/null | sort -hr
find ~ -type f -mtime -1
which find
file /usr/bin/find
file /boot/initrd.img*
